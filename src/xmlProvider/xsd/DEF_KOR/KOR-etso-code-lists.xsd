	<!-- _________________________________________________________ -->
	<xsd:simpleType name="MessageTypeList">
		<xsd:annotation>
			<xsd:documentation>
				<Uid>ET0003 </Uid>
				<Definition> (Synonym "Document Type") The coded type of a document. The document type describes the principal characteristic of a document</Definition>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="A35">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Zgłoszenie problemu</Title>
							<Definition>Dokument wysyłany w przypadku wystąpienia problemów w systemie niezwiązanych bezpośrednio z realizacją procesu biznesowego</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z01">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Zawieszenie realizacji procesu</Title>
							<Definition>Dokument wysyłany w przypadku zawieszenia realizacji podanego procesu biznesowego</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z02">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Zaprzestanie realizacji procesu</Title>
							<Definition>Zaprzestanie przyjmowania do realizacji umów sprzedaży energii i/lub zaprzestanie stosowania ofert bilansujących</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z03">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Wznowienie realizacji procesu</Title>
							<Definition>Dokument wysyłany w przypadku wznowienia realizacji zawieszonego procesu biznesowego</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z04">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Opóźnienie wysłania dokumentów zwrotnych</Title>
							<Definition>Wystąpienie opóźnienia w przekazywaniu przez OSP dokumentów końcowych dla procesu biznesowego</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z05">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Usunięcie problemów związanych z opóźnieniem wysyłania dokumentów zwrotnych</Title>
							<Definition>Usunięcie problemów związanych z opóźnieniem w przekazywaniu przez OSP dokumentów końcowych dla procesu biznesowego</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z06">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Inne informacje</Title>
							<Definition>Dokument wysyłany w przypadku przekazania informacji niezwiązanych z wystąpieniem problemów lub realizacją procesu biznesowego</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z09">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Przesunięcie bramki dla zgłoszeń</Title>
							<Definition>Dokument wysyłany w przypadku zmiany okien czasowych dla dokumentów zgłoszeniowych</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z10">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Otwarcie bramki zgłoszeń USE w ramach RBB w trybie awaryjnym</Title>
							<Definition>Dokument wysyłany w przypadku otwarcia bramki zgłoszeń USE w ramach RBB w trybie awaryjnym</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
