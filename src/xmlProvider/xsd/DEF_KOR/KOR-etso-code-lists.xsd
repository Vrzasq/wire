		<xsd:annotation>
			<xsd:documentation>
				<Uid>ET0005</Uid>
				<Definition>Identification of the role played by a party. </Definition>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="A04">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>System operator</Title>
							<Definition>Przedsiębiorstwo energetyczne posiadające koncesję na przesyłanie i dystrybucję energii elektrycznej, odpowiedzialne za ruch sieciowy sieci przesyłowej na terytorium Rzeczpospolitej Polskiej</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A28">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Scheduling coordinator</Title>
							<Definition>Operator handlowy (OH), Operator handlowo-techniczny (OHT) lub Operator pomiarów (OP) Podmiot, który jest odpowiedzialny za dysponowanie Jednostkami Grafitowymi Uczestnika Rynku Bilansującego.
							</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
