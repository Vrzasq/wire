	<xsd:simpleType name="ProcessTypeList">
		<xsd:annotation>
			<xsd:documentation>
				<Uid>ET0020</Uid>
				<Definition>Indicates the nature of process that the document addresses.</Definition>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="A01">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Proces dnia następnego (ang. Day ahead)</Title>
							<Definition>Informacje w procesie dotyczą dnia następnego.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A02">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Proces przyrostowy dnia bieżącego (ang. Intraday incremental)</Title>
							<Definition>Informacje dotyczą dnia bieżącego, w ramach którego dane są przekazywane przyrostowo.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A05">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Proces agregacji danych pomiarowych (ang. Metered data agregation)</Title>
							<Definition>Przygotowanie danych pomiarowo-rozliczeniowych w trybie wstępnym, podstawowym oraz w trybie korekt.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A06">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Proces rozliczeń rynku bilansującego (ang. Imbalance settlement)</Title>
							<Definition>Rozliczenie dobowe i okresowe rynku bilansującego w trybie wstępnym podstawowym i korygującym.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A15">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Wyznaczanie zdolności przesyłowych (ang. Capacity determination)</Title>
							<Definition>Informacje dotyczące zdolności przesyłowych.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A19">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Proces skumulowany dnia bieżącego (ang. Intraday accumulated)</Title>
							<Definition>Informacje dotyczą dnia bieżącego, w ramach którego są przekazywane skumulowane dane.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A40">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Proces dnia bieżącego (ang. Intraday process)</Title>
							<Definition>Informacje dotyczą dnia bieżącego.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z01">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Proces rozliczenia usług systemowych, kosztów uruchomień oraz dodatkowych kosztów wytwarzania</Title>
							<Definition>Rozliczenie dobowe i okresowe regulacyjnych usług systemowych, kosztów uruchomień oraz dodatkowych kosztów wytwarzania w trybie wstępnym, podstawowym i korygującym.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z04">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Proces przyjmowania zgłoszeń w ramach RBN</Title>
							<Definition>Informacje w procesie przyjmowania zgłoszeń dla dnia następnego.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z05">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Proces przyjmowania zgłoszeń w ramach RBB</Title>
							<Definition>Informacje w procesie przyjmowania zgłoszeń dla dnia bieżącego.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z06">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Proces rynku mocy bilansujących</Title>
							<Definition>Informacje dotyczące rynku mocy bilansujących.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z07">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Proces rozliczania usług systemowych</Title>
							<Definition>Informacje dotyczące rozliczania usług systemowych.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
